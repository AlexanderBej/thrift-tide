    "@nivo/pie": "^0.99.0",
    "@reduxjs/toolkit": "^2.9.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^16.18.126",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "clsx": "^2.1.1",
    "firebase": "^12.4.0",
    "i18next": "^22.5.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-icu": "^2.4.1",
    "lucide": "^0.547.1",
    "lucide-react": "^0.547.0",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.6.0",
    "react-i18next": "^12.3.1",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.4",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "workbox-window": "^7.3.0"
  },
  "scripts": {
    "build:themes": "sass src/styles/theme.light.scss public/light.css && sass src/styles/theme.dark.scss public/dark.css",
    "watch:themes": "sass --watch src/styles/theme.light.scss:public/light.css src/styles/theme.dark.scss:public/dark.css",
    "start": "npm-run-all -p watch:themes start:cra",
    "start:cra": "react-scripts start",
    "build": "npm run build:themes && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@types/jest": "^30.0.0",
    "@types/react-router-dom": "^5.3.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "sass": "^1.93.3"
  }
}
